<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="395.0" prefWidth="535.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.jemylibs.uilib.html.HtmlComponentsSelector">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <HBox spacing="10.0">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Button fx:id="unselectallButton" mnemonicParsing="false" text="اترك الكل"/>
        <Pagination maxPageIndicatorCount="3" pageCount="20"/>
        <Button fx:id="selectallButton" mnemonicParsing="false" text="اختر الكل"/>
    </HBox>
    <TreeView fx:id="treeView" VBox.vgrow="ALWAYS"/>
</VBox>
